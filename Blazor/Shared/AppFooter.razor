@inject IJSRuntime jsRuntime

@code {
    [Parameter]
    public Blazor.Model.AppFooter FooterObj { get; set; }

    protected override void OnAfterRender(bool firstRender) // = On Page Load
    {        
        string str = JsonSerializationHelper.SerializeToJson(FooterObj).ToString();
        jsRuntime.InvokeVoidAsync("setFooter", str, true);
    }
}

